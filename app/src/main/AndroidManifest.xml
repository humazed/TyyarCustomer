<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.tyyar.tyyarfooddelivery"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- <uses-permission android:name="android.permission.INTERNET"/> -->
    <!-- <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/> -->
    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activities.MenuCategoriesActivity">
        </activity>
        <activity android:name=".activities.MenuCategoryItemsActivity">
        </activity>
        <activity android:name=".activities.CartActivity">
        </activity>
        <activity
            android:name=".activities.OrderItemActivity"
            android:label=""
            android:windowSoftInputMode="adjustPan">
        </activity>
        <activity android:name=".activities.CheckoutActivity">
        </activity>
        <activity android:name=".activities.OrdersHistoryActivity">
        </activity>
        <activity android:name=".activities.AccountActivity">
        </activity>
    </application>

</manifest>